}
.main_body .home_container .remember_me_section {
  margin-top: 20px;
  color: #808285;
  font-family: "verdana";
  font-size: 13px;
}
.main_body .footer_contents {
  margin: 28px auto 0;
  width: 410px;
}
.main_body .footer_contents .contect_first {
  float: left;
  color: #808285;
  font-size: 13px;
  font-family: "verdana";
}
.main_body .footer_contents .contect_second {
  float: right;
  color: #808285;
  font-size: 13px;
  font-family: "verdana";
}
